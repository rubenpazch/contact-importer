<h1>imported files</h1>


<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Status</th>
      <th>Total Items</th>
    </tr>
  </thead>
  <tbody>
    <% @uploads.each do |data| %>
      <tr>
        <td><%= data.id %></td>
        <td><%= data.name %></td>
        <td><%=  data.status == 1 ? 'Finished' : 'Failed'  %></td>
        <td><%= data.total_items %></td>
        <td>show details</td>
      </tr>
    <% end %>
  </tbody>
</table>
